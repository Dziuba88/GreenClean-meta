                  svg(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 19.5 19.5"): path(d="M10.2.8l2.5 5.8 6.3.6c.5 0 .6.6.3.8l-4.8 4.2 1.4 6.2c.1.4-.3.8-.7.6l-5.5-3.3L4.3 19a.5.5 0 0 1-.8-.6L5 12.2.2 8a.5.5 0 0 1 .3-.8l6.3-.6L9.3.8c.2-.5.7-.5.9 0z")
                  svg(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 19.5 19.5"): path(d="M10.2.8l2.5 5.8 6.3.6c.5 0 .6.6.3.8l-4.8 4.2 1.4 6.2c.1.4-.3.8-.7.6l-5.5-3.3L4.3 19a.5.5 0 0 1-.8-.6L5 12.2.2 8a.5.5 0 0 1 .3-.8l6.3-.6L9.3.8c.2-.5.7-.5.9 0z")
